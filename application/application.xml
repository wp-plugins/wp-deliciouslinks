<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<application>
		<name>DeliciousLinkSync</name>
		<donate_link>http://wordpress.dcoda.co.uk/donate/deliciouslinksync/
		</donate_link>
		<version>4.1.0a</version>
		<short_desc>Easily synchronize the links list on your blog or multiple
			blogs with the links in your delicious account.</short_desc>
		<tags>synchronize</tags>
		<tags>sync</tags>
		<wpslug>wp-deliciouslinks</wpslug>
		<slug>deliciouslinks</slug>
		<stable_tag>4.0.3</stable_tag>
		<uri>http://wordpress.org/extend/plugins/wp-deliciouslinks/</uri>
		<preload>av23v_delicious</preload>
		<preload>bv23v_http</preload>
		<include>ABSPATH . '/wp-admin/includes/bookmark.php'</include>
		<xml>/library/api/delicious.xml</xml>
		<handler>dlinksyncsettings</handler></application>
	
	<network>
		<id></id><password></password>
		<between></between>
		<last_updated></last_updated></network></settings>
